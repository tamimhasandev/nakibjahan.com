SetEnvIF X-CLUSTER-CLIENT-IP "^185\.183\.107\.164$" DenyAccess

SetEnvIF REMOTE_ADDR "^185\.220\.101\.27$" DenyAccess
SetEnvIF X-FORWARDED-FOR "^185\.220\.101\.27$" DenyAccess
SetEnvIF X-CLUSTER-CLIENT-IP "^185\.220\.101\.27$" DenyAccess

<IfModule mod_litespeed.c>
	Order allow,deny
	Allow from all
	Deny from env=DenyAccess
	Deny from 185.183.107.164
	Deny from 185.220.101.27
